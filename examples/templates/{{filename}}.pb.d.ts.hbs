{{#*inline convert}}
	{{is type "string}}string{{else}}
	{{is type "int32"}}number{{else}}
	{{is type "bool"}}boolean{{else}}
	{{/is}}{{/is}}{{/is}}
{{/inline}}

{{#*inline "package"}}
export namespace {{name}} {
	{{> @partial-block}}
}
{{/inline}}

{{#*inline "message"}}
export interface {{name}} {
	{{> @partial-block}}
}
{{#each nested}}
	{{> package}}
{{/each}}
{{/inline}}

{{#*inline "field"}}
	{{name}}: {{> convert}}
{{/inline}}

{{#*inline "enum"}}
export enum {{name}} {
	{{#each @key}}: "{{name}}",
}
{{/inline}}
